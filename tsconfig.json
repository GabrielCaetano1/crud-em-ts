{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./src",
    
    /* Projects */
    "target": "ESNext",

    /* Modules */
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
